{
  "name": "tourze/json-rpc-endpoint-bundle",
  "description": "JsonRPC解析、处理模块",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "ext-json": "*",
    "monolog/monolog": "^3.1",
    "nesbot/carbon": "^2.72 || ^3",
    "psr/log": "^3|^2|^1",
    "symfony/config": "^7.3",
    "symfony/dependency-injection": "^7.3",
    "symfony/event-dispatcher": "^7.3",
    "symfony/event-dispatcher-contracts": "^3",
    "symfony/http-foundation": "^7.4",
    "symfony/http-kernel": "^7.3",
    "symfony/polyfill-php83": "^1.31",
    "symfony/property-access": "^7.3",
    "symfony/service-contracts": "^3.6",
    "symfony/stopwatch": "^7.3",
    "symfony/validator": "^7.3",
    "tourze/backtrace-helper": "1.*",
    "tourze/bundle-dependency": "1.*",
    "tourze/json-rpc-container-bundle": "2.0.*",
    "tourze/json-rpc-core": "2.0.*",
    "tourze/user-event-bundle": "1.0.*"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^11.5",
    "tourze/phpunit-json-rpc": "1.0.*",
    "tourze/phpunit-symfony-kernel-test": "2.0.*",
    "tourze/phpunit-symfony-unit-test": "2.0.*",
    "tourze/symfony-dependency-service-loader": "1.0.*"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\JsonRPCEndpointBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\JsonRPCEndpointBundle\\Tests\\": "tests"
    }
  }
}
