{
  "name": "tourze/json-rpc-endpoint-bundle",
  "description": "JsonRPC解析、处理模块",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "ext-json": "*",
    "nesbot/carbon": "^2.72 || ^3",
    "symfony/event-dispatcher": "^6.4",
    "symfony/event-dispatcher-contracts": "^2.5 | ^3",
    "symfony/config": "^6.4",
    "symfony/service-contracts": "^3.5",
    "symfony/validator": "^6.4",
    "symfony/stopwatch": "^6.4",
    "symfony/dependency-injection": "^6.4",
    "symfony/http-kernel": "^6.4",
    "symfony/polyfill-php83": "^1.31",
    "tourze/json-rpc-core": "^0.0.6",
    "tourze/json-rpc-container-bundle": "0.0.*",
    "tourze/user-event-bundle": "~0.0.3",
    "tourze/backtrace-helper": "~0.0.5",
    "psr/log": "^3|^2|^1",
    "psr/simple-cache": "^1.0|^2.0|^3.0",
    "psr/container": "^1.1|^2.0",
    "symfony/http-foundation": "^6.4"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\JsonRPCEndpointBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\JsonRPCEndpointBundle\\Tests\\": "tests"
    }
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^10.0"
  }
}